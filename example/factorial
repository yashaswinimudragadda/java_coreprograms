import java.util.Scanner;
import java.lang.*;
import java .io.*;
public class test
{
  public static void main(String[] args) {

    Scanner sc=new Scanner (System.in);

    System.out.print("enter the number(n): ");
    int n=sc.nextInt();
    int i=2;
    int fact=1;
   
    while(i<=n){
        fact=fact*i;
    }
    i++;
    System.out.print("factorial of the given number is "+fact);
    sc.close();

    }
}
