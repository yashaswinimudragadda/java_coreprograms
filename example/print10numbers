import java.util.Scanner;
import java.lang.*;
import java .io.*;
public class test
{
  public static void main(String[] args) {

    Scanner sc=new Scanner (System.in);

    System.out.print("enter the number(n): ");
    int n=sc.nextInt();

    int i=0;
    while(i<=n){
        System.out.println(i);
        i++;
    }
    

    }
}
