import java.util.Scanner;
import java.lang.*;
import java .io.*;
public class test
{
  public static void main(String[] args) {

    Scanner sc=new Scanner (System.in);

    System.out.print("enter the number(n): ");
    int n=sc.nextInt();

    int i=1;
 
    while(i<=n){
           int j=1;
        do{
       System.out.println(i+"X"+j+"="+(i*j));
       j++;

        }while(j<=n);
        System.out.println("----------");
        i++;
    }
    sc.close();

    }
}
