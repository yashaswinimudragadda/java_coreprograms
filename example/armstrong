class Main {
    public static void main(String[] args) {
        
		int n=153;
		int i=0;
		int s=0;
		int r=0;
		int t=0;
		t=n;
		while(i<n) {
			r=n%10;
			s=s+(r*r*r);
			n=n/10;
		}
		i++;
		if(t==s) {
			System.out.println("argstrong");
				
		}
    }
}
